<div class="container">
  <div class="row">
    <div class="col-sm-4 col-xs-5">
      <div class="profile_blurb">
        <%= @user.blurb %>
      </div>
    </div>
    <div class="col-sm-8 col-xs-7">
      <h1 class="green alt_font no_margin_top"><%= @user.first_name %> <%= @user.last_name %></h1>
      <h2><%= @user.title %></h2>
      <div id="profile_job_timeline">
        job timeline
      </div>
  </div>
  <div class="row">
    <div class="col-sm-4">
      <h3>Skills</h3>
    </div>
    <div class="col-sm-4">
      <h3>Code Samples</h3>
    </div>
    <div class="col-sm-4">
      <h3>Preferences</h3>
    </div>
  </div>
</div>